<h1>Check out who you can hangout with</h1>

<ul class = "col-md-6">
  <li ng-repeat="overlap in main.Times.overlap_list">
  <h3>{{overlap[3]}}</h3>
    {{ overlap[0] | date : 'medium'}} to {{overlap[1] | date : 'medium'}}
  </li>
</ul>

<div class = "col-md-6">
    <h2>Check who is free</h2>
    <div class="container">
      <div class="row">
          <div class='col-sm-6'>
              <div class="form-group">
                  <div class='input-group date' id='datetimepicker1'>
                      <input type='text' class="form-control" id="timer"/>
                      <span class="input-group-addon"><span class="glyphicon glyphicon-calendar"></span>
                      </span>
                  </div>
              </div>
          </div>
          <script type="text/javascript">
              $(function () {
                  $('#datetimepicker1').datetimepicker();
              });
          </script>
      </div>
      <button ng-click="main.Times.compare()">Check</button>
    </div>
<!-- blow displays the users and timeslots that intersect -->
      <ul>
        <li ng-repeat="time in main.Times.intersect_times">
          <h2>{{time[3]}}</h2>
          {{time[0] | date:"medium"}}
          to
          {{time[1] | date:"medium"}}
        </li>
      </ul>
     <div>
      
    </div>
</div>
